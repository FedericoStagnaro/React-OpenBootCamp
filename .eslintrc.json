{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "globals": {
        "document": true
    },
    "settings": {
        "react": {
            "version": "detect"
        },
        "overrides": [
            {
                "files": [ "*.js" , "*.jsx"]
            }
        ]
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": 0,
        "react/prop-types": 0,
        "linebreak-style": 0
    }
}
